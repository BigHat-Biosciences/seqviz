{
  "name": "seqviz",
  "version": "3.0.38",
  "private": false,
  "description": "A DNA sequence viewer supporting custom, GenBank, FASTA, NCBI accession, and iGEM input",
  "scripts": {
    "build": "rm -rf ./dist && webpack --config ./config/webpack.config.js --display-error-details --verbose",
    "demo": "cd demo && npm run deploy && cd .. && git add . && git commit -m \"Update demo\"",
    "format": "npx prettier ./src/**/* --config package.json --write",
    "markdown": "node ./config/scripts/markdown.js ./README.md",
    "update": "npm run build && npm run markdown && git add . && git commit --amend -C HEAD && npm publish",
    "major": "npm run test:ci && npm version major && npm run update",
    "minor": "npm run test:ci && npm version minor && npm run update",
    "patch": "npm run test:ci && npm version patch && npm run update",
    "start": "PORT=8000 node ./config/scripts/start.js",
    "test": "node ./config/scripts/test.js",
    "test:ci": "CI=true npm run test && npm run build"
  },
  "keywords": [
    "seqviz",
    "DNA",
    "viewer",
    "sequence viewer",
    "vector viewer",
    "sequence visualizer",
    "plasmid",
    "plasmid viewer"
  ],
  "homepage": "https://github.com/Lattice-Automation/seqviz",
  "bugs": {
    "url": "https://github.com/Lattice-Automation/seqviz/issues",
    "email": "contact@latticeautomation.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lattice-Automation/seqviz.git"
  },
  "license": "MIT",
  "author": "Lattice Automation (https://latticeautomation.com/)",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "unpkg": "dist/seqviz.min.js",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "babel-plugin-module-resolver",
      "babel-plugin-transform-imports"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:prettier/recommended",
      "prettier/react",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2016,
      "sourceType": "module"
    },
    "plugins": [
      "prettier",
      "react"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/prop-types": 0
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "verbose": true,
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "bufferpack": "0.0.6",
    "node-fetch": "^2.6.1",
    "prop-types": "^15.7.2",
    "react-sizeme": "^2.6.12",
    "string_decoder": "^1.3.0",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@svgr/webpack": "^4.3.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.1.2",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "core-js": "^3.7.0",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.3.0",
    "file-loader": "^4.3.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^26.6.2",
    "jest-watch-typeahead": "^0.6.1",
    "mini-css-extract-plugin": "^0.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "prettier": "^1.19.1",
    "react": "^16.14.0",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^11.0.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^4.0.0",
    "regenerator-runtime": "^0.13.7",
    "resolve": "^1.19.0",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^8.0.2",
    "semver": "^6.3.0",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "2.2.1",
    "ts-pnp": "^1.2.0",
    "typescript": "^3.9.7",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^2.3.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^4.3.1",
    "xml-comment-api": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "contributors": [
    {
      "name": "Joshua Timmons",
      "email": "jtimmons@latticeautomation.com",
      "url": "https://github.com/JJTimmons"
    },
    {
      "name": "Kevin LeShane",
      "email": "kevin@latticeautomation.com",
      "url": "https://github.com/leshane"
    },
    {
      "name": "Raymond Chung",
      "url": "https://github.com/RazzYoshi"
    }
  ]
}
